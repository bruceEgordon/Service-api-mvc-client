@model EPiServer.Integration.Client.Models.Catalog.Entry

@{
    ViewBag.Title = "Create Entry";
}

<h2>Create Entry</h2>

@using (Html.BeginForm("SubmitEntry", "Home"))
{
    <h4>Entry Data</h4>
    <div class="container">
        <div class="form-row">
            <div class="form-group col-2">
                <label>Code</label>
                @Html.EditorFor(m => m.Code, new { htmlAttributes = new { @class = "form-control" } })
            </div>
            <div class="form-group col-2">
                <label>Name</label>
                @Html.EditorFor(m => m.Name, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
    </div>
}